{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/games/tic-tac-toe/(.*)", "dest": "/games/tic-tac-toe/$1" },
    { "src": "/games/snake/(.*)", "dest": "/games/snake/$1" }
  ]
}
